# Implementation of Ranges


csa:HeartAttackRange owl:equivalentClass [
        a owl:Restriction ;
          owl:onProperty csa:hasHeartRate ;
          owl:someValuesFrom [
              a rdfs:Datatype ;
              owl:onDatatype xsd:integer ;
              owl:withRestrictions ( [ xsd:minInclusive 140 ] )
          ]
] .


csa:CardiacArrestRange owl:equivalentClass [
         a owl:Restriction ;
          owl:onProperty csa:hasHeartRate ;
          owl:someValuesFrom [
              a rdfs:Datatype ;
              owl:onDatatype xsd:integer ;
              owl:withRestrictions ( [ xsd:maxInclusive 30 ] )
          ]

] .

