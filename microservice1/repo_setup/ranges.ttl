###

csa:BasicCertification owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty csa:enablesToHandleEmergency ;
    owl:someValuesFrom [
        a owl:Restriction ;
        owl:onProperty csa:hasSeverity ;
        owl:hasValue 1
    ]
] .

csa:IntermediateCertification owl:equivalentClass [
    a owl:Class ;
    owl:unionOf (
        [ a owl:Restriction ;
          owl:onProperty csa:enablesToHandleEmergency ;
          owl:someValuesFrom [
              a owl:Restriction ;
              owl:onProperty csa:hasSeverity ;
              owl:hasValue 2
          ]
        ]
        [ a owl:Restriction ;
          owl:onProperty csa:enablesToHandleEmergency ;
          owl:someValuesFrom [
              a owl:Restriction ;
              owl:onProperty csa:hasSeverity ;
              owl:hasValue 3
          ]
        ]
    )
] .

csa:AdvancedCertification owl:equivalentClass [
    a owl:Class ;
    owl:unionOf (
        [ a owl:Restriction ;
          owl:onProperty csa:enablesToHandleEmergency ;
          owl:someValuesFrom [
              a owl:Restriction ;
              owl:onProperty csa:hasSeverity ;
              owl:hasValue 4
          ]
        ]
        [ a owl:Restriction ;
          owl:onProperty csa:enablesToHandleEmergency ;
          owl:someValuesFrom [
              a owl:Restriction ;
              owl:onProperty csa:hasSeverity ;
              owl:hasValue 5
          ]
        ]
    )
] .


